<h2>Users search for <%= range = params[:word] %> </h2>
<table class = "table table-hover table-inverse" >
  <tbody>
    <tr>
      <td></td>
      <td>Name</td>
      <td></td>
    </tr>
    <% users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image, size: '50x50' %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user, class: "user_#{user.id}" %></td>
    </tr>
    <% end %>
  </tbody>
</table>